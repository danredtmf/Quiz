; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Quiz"
run/main_scene="res://scenes/loading_game/loading_game.tscn"
config/icon="res://icon.png"
config/version="1.0"

[autoload]

Data="*res://global_scripts/data.gd"
Core="*res://global_scripts/core.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="expand"

[locale]

translations=PoolStringArray( "res://resourses/locale/locale.en.translation", "res://resourses/locale/locale.eo.translation", "res://resourses/locale/locale.ru.translation" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
